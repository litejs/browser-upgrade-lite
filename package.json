{
  "name": "browser-upgrade-lite",
  "version": "0.1.8",
  "stability": 2,
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "Implement EcmaScript 5 methods for older browsers",
  "keywords": [
    "browser",
    "litejs"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/litejs/browser-upgrade-lite.git"
  },
  "bugs": {
    "url": "https://github.com/litejs/browser-upgrade-lite/issues"
  },
  "devDependencies": {
    "buildman": "*",
    "testman": "*"
  },
  "buildman": {
    "min.js": "index.js",
    "min.base64.js": "base64.js",
    "min.array.js": "array.js"
  },
  "testling": {
    "files": "tests/run.js",
    "browsers": {
      "ie": [
        6,
        7,
        8,
        9,
        10
      ],
      "ff": [
        3,
        4,
        5,
        20,
        25,
        "nightly"
      ],
      "chrome": [
        4,
        10,
        20,
        30,
        31,
        "canary"
      ],
      "safari": [
        4,
        5.1,
        6
      ],
      "opera": [
        10,
        11,
        "11.6",
        12,
        "next"
      ],
      "android-browser": [
        4.2
      ],
      "iphone": [
        6
      ],
      "ipad": [
        6
      ]
    }
  }
}
